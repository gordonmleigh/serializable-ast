{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["es2015"],
    "module": "ES2020",
    "moduleResolution": "node",
    "noErrorTruncation": true,
    "noUnusedLocals": true,
    "outDir": "lib/",
    "pretty": true,
    "rootDir": "generate/",
    "sourceMap": true,
    "strict": true,
    "target": "ES2020"
  },
  "include": ["generate/"],
  "exclude": ["lib/", "node_modules/"]
}
